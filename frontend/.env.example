# =============================================================================
# LUMILINK FRONTEND ENVIRONMENT VARIABLES
# =============================================================================
# Copy this file to .env and fill in your actual values

# Cloudinary Configuration for Video Uploads
REACT_APP_CLOUDINARY_CLOUD_NAME=your-cloudinary-cloud-name
REACT_APP_CLOUDINARY_UPLOAD_PRESET=lumilink_videos
REACT_APP_CLOUDINARY_API_KEY=your-api-key
REACT_APP_CLOUDINARY_API_SECRET=your-api-secret

# API Configuration
REACT_APP_API_URL=http://localhost:3001/api/v1

# Other configurations
REACT_APP_ENVIRONMENT=development
